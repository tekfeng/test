<div class='row'>
  <div class='col-lg-12'>
    <div class="row wrapper border-bottom white-bg page-heading">
      <div class="col-lg-12">
        <div class="col-md-4 col-sm-4 col-xs-12">
          <div class="new-lead">
            <%= link_to 'New Booking', new_booking_url, class: "btn btn-primary pull-left", id: "new" %>
          </div>
        </div>
        <div class="col-md-4 col-sm-4 col-xs-12 text-center">
          <h2>Booking</h2>
          <ol class="breadcrumb">
            <li>
              <a href="/dashboard">Home</a>
            </li>
            <li>
              <a href="/dashboard">Sales</a>
            </li>
            <li class="active">
              <strong>Booking</strong>
            </li>
          </ol>
        </div>
        <div class="col-md-4 col-sm-4 col-xs-12">
          <form class="group-input">
            <input type='search' id='search-customer' placeholder="Search..." />
            <a class="submit" type="submit" href="#">
              <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
            </a>
          </form>
        </div>
      </div>
    </div>
    
    <div class='wrapper wrapper-content'>
      <div class='ibox'>
        <div class='ibox-content container-fluid white-bg'>
          <div id="vendor_filter">
          </div>
        </div>
        <div id="listing-table">
        </div>
      </div>
    </div>
  </div>
</div>

   


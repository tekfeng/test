<table class='listing-table vendor-listing-content-tab-vendor-rates'>
  <thead>
    <tr>
      <th class="booking-number">Date
        <a class="menu-down" href="#">
          <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
        </a>
      </th>
      <th class="booking-travel-date">Name
        <a class="menu-down" href="#">
          <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
        </a>
      </th>
      <th class="booking-date">Email Address
        <a class="menu-down" href="#">
          <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
        </a>
      </th>
      <th class="booking-name">Source
        <a class="menu-down" href="#">
          <span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span>
        </a>
      </th>
       <th class="booking-action" style='text-align: center;background:#ececec;'>
       </th>
    </tr>
  </thead>
  <% if smart_listing.collection.count > 0 %>
    <% smart_listing.collection.each_with_index do |incoming, index| %>
      <tr class="<%= cycle('odd', 'even') %>">
        <td><%=  incoming.created_at.strftime("%Y-%-m-%d") %></td>
        <td><%=  incoming.customer_name %></td>
        <td><%=  incoming.email_address %></td>
        <td>sample@source</td>
        <td style='text-align: center;'>
          <%= link_to "View", "/incomings/#{incoming.id}", class: 'btn btn-primary' %>
        </td>
      </tr>
    <% end %>
  <% else %>
    <tr>
      <td colspan='6'>No Incoming available<td>
    </tr>
  <% end %>
</table>
<%= smart_listing.paginate  %>
